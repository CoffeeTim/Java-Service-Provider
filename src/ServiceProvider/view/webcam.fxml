<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ServiceProvider.controllers.WebcamController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <ImageView fx:id="image" fitHeight="283.0" fitWidth="324.0" layoutX="229.0" layoutY="58.0" pickOnBounds="true" preserveRatio="true" />
        <Button fx:id="btnCapture" layoutX="237.0" layoutY="426.0" mnemonicParsing="false" onMouseClicked="#btnCaptureClicked" prefHeight="31.0" prefWidth="156.0" text="Capture" />
        <Button fx:id="btnSave" layoutX="427.0" layoutY="426.0" mnemonicParsing="false" onMouseClicked="#btnSaveClicked" prefHeight="31.0" prefWidth="156.0" text="Save" />
    </children>
</AnchorPane>
